{
  "name": "austingoudge/marinenotice",
  "description": "marinenotice.net configuration.",
  "require": {
    "psycle-wordpress/wordpress": "*",
    "wpackagist-plugin/w3-total-cache": "*",
    "wpackagist-plugin/theme-my-login": "7.0.15",
    "wpackagist-plugin/wp-user-avatar": "2.2.5",
    "wpackagist-plugin/svg-support": "2.3.17",
    "wpackagist-plugin/post-expirator": "2.4.0.1",
    "wpackagist-plugin/tinymce-advanced": "5.3.0",
    "wpackagist-plugin/cookie-notice": "1.2.50"
  },
  "extra": {
    "installer-paths": {
      "custom/mu-plugins/{$name}": [
        "type:wordpress-muplugin"
      ],
      "custom/plugins/{$name}/": [
        "type:wordpress-plugin"
      ],
      "custom/themes/{$name}/": [
        "type:wordpress-theme"
      ]
    },
    "wordpress-install-dir": {
      "wordpress/wordpress": "wp"
    }
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://gitlab.psycle.com/psycle-wordpress/wordpress.git"
    },
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "package",
      "package": {
        "name": "wordpress/wordpress",
        "type": "wordpress-core",
        "version": "5.3.2",
        "source": {
          "type": "git",
          "url": "https://github.com/WordPress/WordPress.git",
          "reference": "5.3.2"
        }
      }
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true
}